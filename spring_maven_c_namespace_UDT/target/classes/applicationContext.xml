<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:c="http://www.springframework.org/schema/c"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
 http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="student" class="com.zensar.spring.beans.Student"
		c:studentId="200" c:studentName="afasfa" c:studentMarks="85" c:address-ref="addr">
	</bean>
	
	<bean id="addr" class="com.zensar.spring.beans.Address"
	c:pincode="452396" c:city="goa" c:state="mp">
	</bean>


</beans>