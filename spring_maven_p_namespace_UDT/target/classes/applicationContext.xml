<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:c="http://www.springframework.org/schema/c"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
 http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="student" class="com.zensar.spring.beans.Student"
		p:studentId="108" p:studentName="XYZ" p:studentMarks="55" p:address-ref="addr">
	</bean>
	
	<bean id="addr" class="com.zensar.spring.beans.Address"
	p:pincode="452396" p:city="inodre" p:state="mp">
	</bean>


</beans>